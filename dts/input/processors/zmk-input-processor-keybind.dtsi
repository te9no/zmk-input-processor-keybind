#include <zephyr/dt-bindings/input/input-event-codes.h>
#include <dt-bindings/zmk/keys.h>

/ {
    /* decompress z to xy with key mapping */
    /omit-if-no-ref/ zip_keybind_keys: zip_keybind_keys {
        compatible = "zmk,input-processor-keybind";
        #input-processor-cells = <0>;  // No parameters needed
        type = <INPUT_EV_REL>;
        track-remainders;
        key-codes = <UP DOWN LEFT RIGHT>;  // デフォルト値
    };
};